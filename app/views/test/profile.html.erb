<div class="profile-page">
	<% if current_user == @user %>
		<%= render "status_update" %>
	<% end %>

	<h2 class="header orange-text center"><%= @user.first_name %>'s Profile</h2> 
	<% if current_user != @user %>
		<%= render "friendprofile" %>
	<% end %>
	<%=  render "activities/userfeed" %>

</div>	