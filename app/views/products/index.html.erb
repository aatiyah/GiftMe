<div class="container">
	<div class="row">
		<% @product.each do |p|%>
			<div class="thumbnail col s12 m4 l3">
				<img src="<%=p.img_url%>">
				<p><%=link_to p.title, item_path(p.category_id, p.id)%></p>
				<p><%=p.price%></p>
			</div>
		<% end %>

	</div>
</div>

<%=will_paginate(@product, :renderer => WillPaginate::ActionView::LinkRenderer)  %>
