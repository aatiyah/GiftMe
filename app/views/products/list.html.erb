<div class="container">
	<div class="row">
		<% @products.each do |p| %>
			<div class="thumbnail col s12 m4 l3">
				<img src="<%=p.img_url%>">
				<div class= "textWrapper">
					<p><%=link_to p.title, item_path(p.category_id, p.id)%></p>
					<p><%=p.price%></p>
				</div>
			</div>
		<% end %>
	</div>
</div>

<%=will_paginate(@products, :renderer => WillPaginate::ActionView::LinkRenderer)  %>

