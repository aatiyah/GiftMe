<div class="container">
	<div class="card" id="search-card">
		<div class="search">
			<%=form_tag products_search_path, :method => 'get' do %>
				<div class="input-field">
					<%= text_field_tag :search, params[:search] %>
					<label for="search"><i class=" material-icons">search</i></label>
				</div>
			<% end %>
		</div>
	</div>
</div>	
